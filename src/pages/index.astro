---
import Layout from "@layouts/Layout.astro";
import Posts from "@organisms/Posts/Tutoriales/Tutoriales.astro";
import DividerTop from "@atoms/DividerTop/DividerTop.astro";
import Hero from "@organisms/Hero/Hero.astro";
import DesarrolloWeb from "@organisms/Posts/DesarrolloWeb/DesarrolloWeb.astro";
import { getCategoriesWithPosts } from "@services/categoriesWithPosts";
import DividerBottom from "@src/components/atoms/DividerBottom/DividerBottom.astro";
const categoriesWithPosts = await getCategoriesWithPosts();
export const prerender = true;




---

<Layout>
  <Hero />
  <DividerTop />
  <Posts {...categoriesWithPosts["tutoriales"][0]} />
  <DividerTop  fill="shape-fill-white"/>
  <DesarrolloWeb  {...categoriesWithPosts["aprender-a-programar"][0]}/>
  <DividerBottom />


</Layout>