---
import type { Block } from '@wp-block-tools/styles';
type Props = {
    blocks: Block[];
}
const {blocks} = Astro.props



---
<div>
    {
        blocks.map( (block) => {
            switch (block.name) {
                case "core/post-content":
                    return <div> post content</div>
                    break;
                default:
                    return null;
                    break;
            }
    })
    }
</div>



